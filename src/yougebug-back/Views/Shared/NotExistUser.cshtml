
<div class="row">
    <div class="col-2 d-none d-lg-block" style="width: 150px;">
        @await Html.PartialAsync("_LeftNav")
    </div>

    <div class="col-sm-12 col-lg-8">
        <div class="text-center mt-5">
            <h4>@@<span id="spn_name"></span></h4>
        </div>
        <div class="text-center mt-1">
            <h4>这个账号不存在</h4>
        </div>
    </div>

    <div class="col-2 d-none d-lg-block">
        @await Html.PartialAsync("_RightNav")
    </div>
</div>
@section Scripts {
    <script>
        document.getElementById('spn_name').innerHTML = location.pathname.replace('/', '');
    </script>
}